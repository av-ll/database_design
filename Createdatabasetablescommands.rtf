  CREATE TABLE host_rating_guest (\
  h_rating_id INT AUTO_INCREMENT,\
  stars TINYINT(1) NOT NULL,\
  comment TEXT,\
  host_rater INT NOT NULL,\
  guest_rated INT NOT NULL,\
