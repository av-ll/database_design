  type VARCHAR(25) NOT NULL,\
  start_date DATE,\
  end_date DATE,\
  limit_use TINYINT(2),\
  voucher_code VARCHAR(8) UNIQUE NOT NULL,\
  PRIMARY KEY(action_id)\
  );\
