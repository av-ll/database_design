  password CHAR(64) NOT NULL,\
  last_login TIMESTAMP,  \
  user_id INT UNIQUE NOT NULL,\
  PRIMARY KEY(login_id),\
  FOREIGN KEY(user_id) REFERENCES user(user_id)\
  ON DELETE CASCADE ON UPDATE CASCADE \
  );\
