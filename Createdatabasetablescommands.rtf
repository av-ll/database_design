CREATE TABLE payment_info (\
  payment_info_id INT AUTO_INCREMENT,\
  type CHAR(10) NOT NULL,\
  user_id INT NOT NULL,\
  PRIMARY KEY(payment_info_id),\
  FOREIGN KEY(user_id) REFERENCES user(user_id)\
