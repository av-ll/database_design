CREATE DATABASE AIRBNB;\
USE AIRBNB;\
CREATE TABLE user  (\
  user_id INT AUTO_INCREMENT,\
  type CHAR(10) NOT NULL,\
  name VARCHAR(50) NOT NULL,\
  surname VARCHAR(50) NOT NULL,\
  gender CHAR(1),\
  photo MEDIUMBLOB,\
  date_of_birth DATE NOT NULL,\
  join_date DATE NOT NULL,\
  country CHAR(2) NOT NULL,\
  PRIMARY KEY(user_id)\
